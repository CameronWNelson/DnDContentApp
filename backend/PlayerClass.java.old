public enum PlayerClass {
    BARBARIAN("Barbarian"),
    BARD("Bard"),
    CLERIC("Cleric"),
    DRUID("Druid"),
    FIGHTER("Fighter"),
    MONK("Monk"),
    PALADIN("Paladin"),
    RANGER("Ranger"),
    ROGUE("Rogue"),
    SORCERER("Sorcerer"),
    WARLOCK("Warlock"),
    WIZARD("Wizard");

    private String playerClass;

    PlayerClass(String playerClass)
    {
        this.playerClass = playerClass;
    }

    public String getClassName()
    {
        return this.playerClass;
    }
}